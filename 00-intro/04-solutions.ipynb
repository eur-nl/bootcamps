   "source": [
    "# this one uses a for loop\n",
    "def dedupe_v1(x):\n",
    "  y = []\n",
    "  for i in x:\n",
    "    if i not in y:\n",
    "      y.append(i)\n",
    "  return y\n",
    "\n",
    "#this one uses sets\n",
    "def dedupe_v2(x):\n",
    "    return list(set(x))\n",
    "\n",
    "a = [1,2,3,4,3,2,1]\n",
    "print(a)\n",
    "print(dedupe_v1(a))\n",
    "print(dedupe_v2(a))"
   ]
  }
